{
  "name": "hackaton-lookfor",
  "version": "1.0.0",
  "description": "Self-referential meta-automata for MAS generation",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/api/server.ts",
    "start": "node dist/api/server.js",
    "demo": "tsx scripts/demo.ts",
    "mock-api": "tsx scripts/start-mock-api.ts",
    "evaluate": "USE_MOCK_API=true tsx scripts/evaluate.ts",
    "evaluate:prod": "tsx scripts/evaluate.ts",
    "judge": "tsx scripts/judge-suite.ts",
    "audit": "tsx scripts/auditor.ts --once",
    "audit:loop": "tsx scripts/auditor.ts",
    "semantic": "tsx scripts/semantic-test.ts",
    "debug:api": "tsx scripts/debug-api.ts",
    "test:api": "tsx scripts/test-api.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "dotenv": "^17.2.4"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/logicsticks/Lookfor_Hackathon_2026_logicsticks"
  }
}
